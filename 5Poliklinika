using System;

namespace romchik
{
    class Program
    {
        static void Main(string[] args)
        {
            int timeUnitMinutes = 10;
            string starushka = " ";
            int inputStarushka = 0;
            int min;

            Console.Write("Введите кол-во старушек: ");
            starushka = Console.ReadLine();
            int.TryParse(starushka, out inputStarushka);

            min = inputStarushka * timeUnitMinutes;
            int hourUnit = min / 60;
            int minRemainder = min % 60;
            Console.WriteLine($"Вы должны отстоять в очереди {hourUnit} часов и {minRemainder} минут");
        }
    }
}
